# Application
APP_NAME=LicenseManagement
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost:8000
APP_TIMEZONE=UTC

# Logging
LOG_CHANNEL=stack
LOG_SLACK_WEBHOOK_URL=

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=license_local
DB_USERNAME=root
DB_PASSWORD=

# Cache
CACHE_DRIVER=file
QUEUE_CONNECTION=sync

# JWT Authentication
JWT_SECRET=your-jwt-secret-key-here
JWT_TTL=60
JWT_REFRESH_TTL=20160

# Mail Configuration
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=noreply@example.com
MAIL_FROM_NAME="${APP_NAME}"

# Stripe
STRIPE_KEY=your-stripe-public-key
STRIPE_SECRET=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# OptumAdmin Integration
OPTUMADMIN_API_KEY=your-optumadmin-api-key-here
OPTUMADMIN_WEBHOOK_URL=https://us-central1-optum-80593.cloudfunctions.net/vmUsageWebhook

# Firebase Cloud Functions Integration
FIREBASE_FUNCTIONS_URL=https://us-central1-optum-80593.cloudfunctions.net
FIREBASE_API_KEY=your-firebase-api-key-here
